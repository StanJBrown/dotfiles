#bin/bash
# A bulk TV series renaming script
#
# author: Chris Choi <chutsu@gmail.com>
echo "What extension are the videos?(ignoring the first dot)"
read extension
for i in *.$extension; do
	echo "$i"
	echo "Episode name:"
	read epiTitle 
	echo ""
	mv "$i" "${i/.$extension/}$epiTitle.$extension"	
done
